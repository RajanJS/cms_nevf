    "express-rate-limit": "^5.1.1",
    "firebase": "^7.13.1",
    "firebase-admin": "^8.10.0",
    "helmet": "^3.22.0",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4"
